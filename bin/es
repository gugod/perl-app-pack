  #===================================
  sub delete_by_query {
  #===================================
      shift()->_do_action(
          'delete_by_query',
          {   %Search_Defn,
              method  => 'DELETE',
              postfix => '_query',
              qs      => {
                  consistency => CONSISTENCY,
                  replication => REPLICATION,
                  routing     => ['flatten'],
              },
              %Query_Defn,
              fixup => sub {
                  _query_fixup(@_);
                  die "Missing required param 'query' or 'queryb'\n"
                      unless %{ $_[1]->{data} };
              },
          },
          @_
      );
  }
  
