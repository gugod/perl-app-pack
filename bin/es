  ## PERCOLATOR
  ##################################
  #===================================
  sub create_percolator {
  #===================================
      shift()->_do_action(
          'create_percolator',
          {   cmd    => CMD_INDEX_PERC,
              prefix => '_percolator',
              method => 'PUT',
              data   => {
                  query  => ['query'],
                  queryb => ['queryb'],
                  data   => ['data']
              },
              fixup => sub {
                  my $self = shift;
                  my $args = shift;
                  $self->_to_dsl( { queryb => 'query' }, $args->{data} );
                  die('create_percolator() requires either the query or queryb param'
                  ) unless $args->{data}{query};
                  die 'The "data" param cannot include a "query" key'
                      if $args->{data}{data}{query};
                  $args->{data} = {
                      query => $args->{data}{query},
                      %{ $args->{data}{data} }
                  };
              },
          },
          @_
      );
  }
  
  #===================================
  sub delete_percolator {
  #===================================
      shift()->_do_action(
          'delete_percolator',
          {   cmd    => CMD_INDEX_PERC,
