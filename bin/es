          version_type => [ 'enum', [ 'internal', 'external' ] ],
      },
      data  => { data => 'data' },
      fixup => sub {
          my $data = $_[1]{data}{data};
          $_[1]{data} = ref $data eq 'HASH' ? $data : \$data;
      }
  );
  
