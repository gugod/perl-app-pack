  # every time we setup a connection
  my %SSL_OP_NO;
  for(qw( SSLv2 SSLv3 TLSv1 TLSv1_1 TLSv11:TLSv1_1 TLSv1_2 TLSv12:TLSv1_2 )) {
      my ($k,$op) = m{:} ? split(m{:},$_,2) : ($_,$_);
      my $sub = "Net::SSLeay::OP_NO_$op";
      local $SIG{__DIE__};
      $SSL_OP_NO{$k} = eval { no strict 'refs'; &$sub } || 0;
  }
  
