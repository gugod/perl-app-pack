$fatpacked{"HTTP/Entity/Parser/JSON.pm"} = '#line '.(1+__LINE__).' "'.__FILE__."\"\n".<<'HTTP_ENTITY_PARSER_JSON';
  package HTTP::Entity::Parser::JSON;
  
  use strict;
  use warnings;
  use JSON::MaybeXS qw/decode_json/;
  use Encode qw/encode_utf8/;
  
  sub new {
      bless [''], $_[0];
  }
  
  sub add {
      my $self = shift;
      if (defined $_[0]) {
          $self->[0] .= $_[0];
      }
  }
  
  sub finalize {
      my $self = shift;
  
      my $p = decode_json($self->[0]);
      my @params;
      if (ref $p eq 'HASH') {
          while (my ($k, $v) = each %$p) {
              if (ref $v eq 'ARRAY') {
                  for (@$v) {
                      push @params, encode_utf8($k), encode_utf8($_);
                  }
              } else {
                  push @params, encode_utf8($k), encode_utf8($v); 
              }
          }
      }
      return (\@params, []);
  }
  
  1;
  
  __END__
  
