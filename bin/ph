                    if(defined $_[0] && $_[0]>0) {
  				if($_[0]) {return undef if &selectfile($File,$_[0])==0}
  		    }
  			if(defined $_[0] && $_[0]<0) {&setnodelay($File,1);}
  			my($value) = getc $File;
  			if(defined $_[0] && $_[0]<0) {&setnodelay($File,0);}
  			$value;
  		}
  		sub ReadLine {
  		  my($File) = normalizehandle((@_>1?$_[1]:\*STDIN));
  		    if(defined $_[0] && $_[0]>0) {
