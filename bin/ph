  sub remove_member {
      my ( $self, %args ) = @_;
      carp '"Remove team member" API is deprecated. Use remove_membership method.';
      croak 'Missing key in parameters: team_id' unless $args{team_id};
      croak 'Missing key in parameters: user'    unless $args{user};
      return $self->request(
