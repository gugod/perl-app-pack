      'GET /user',
      'GET /user/emails',
      'GET /user/followers',
      'GET /user/following',
      'GET /user/keys',
      'GET /user/repos',
      'PATCH /user',
      'POST /user/emails',
      'POST /user/keys',
      'POST /user/repos',
  );
  
