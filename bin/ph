  		    die "Incorrect Type : the argument must be an array or hash reference";
  		}
  	}
  	else {
  		$_array = \@array;
  	}
  	my $iterator = {
          _current_index => 0,
          _length => 0,
          _iteratee => [],
          _iterated => 0,
          };
  	bless($iterator, $class);
  	$iterator->_init(scalar(@{$_array}), $_array);
  	return $iterator;
  }
  
