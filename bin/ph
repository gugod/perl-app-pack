                       }
  		  }
                    if (defined $_[0] && $_[0] < 0) {
                       &setnodelay($File,1)
                    };
                    my ($value) = scalar(<$File>);
                    if ( defined $_[0] && $_[0]<0 ) {
                      &setnodelay($File,0)
                    };
                    $value;
  		}
  DONE
      }
      elsif ( &blockoptions() & 4 )    #select
      {
          eval <<'DONE';
  		sub ReadKey {
