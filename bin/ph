      my $p = Pithub::PullRequests->new;
      my $result = $p->files(
          user            => 'plu',
          repo            => 'Pithub',
          pull_request_id => 1,
      );
  
  =back
  
  =head2 get
  
  =over
  
  =item *
  
  Get a single pull request
  
      GET /repos/:user/:repo/pulls/:id
  
