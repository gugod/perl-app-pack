$fatpacked{"Pithub.pm"} = '#line '.(1+__LINE__).' "'.__FILE__."\"\n".<<'PITHUB';
  package Pithub;
  our $AUTHORITY = 'cpan:PLU';
  our $VERSION = '0.01034';
  # ABSTRACT: Github v3 API
  
  use Moo;
  use Pithub::Events;
  use Pithub::Gists;
  use Pithub::GitData;
  use Pithub::Issues;
  use Pithub::Orgs;
  use Pithub::PullRequests;
