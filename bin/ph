  package Pithub::Repos::Collaborators;
  our $AUTHORITY = 'cpan:PLU';
  our $VERSION = '0.01034';
  # ABSTRACT: Github v3 Repo Collaborators API
  
  use Moo;
  use Carp qw(croak);
  extends 'Pithub::Base';
  
  
  sub add {
