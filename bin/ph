                        return undef if &pollfile($File,$_[0]) == 0;
                      }
  		  }
                    if (defined $_[0] && $_[0] < 0) {
                       &setnodelay($File,1);
                    }
                    my ($value) = getc $File;
                    if (defined $_[0] && $_[0] < 0) {
                       &setnodelay($File,0);
                    }
                    $value;
  		}
  		sub ReadLine {
  		  my($File) = normalizehandle((@_>1?$_[1]:\*STDIN));
  
