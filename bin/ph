  package LWP::MemberMixin;
  
  our $VERSION = '6.34';
  
  sub _elem {
      my $self = shift;
      my $elem = shift;
      my $old  = $self->{$elem};
      $self->{$elem} = shift if @_;
      return $old;
  }
  
  1;
  
