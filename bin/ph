  sub _init {
  	my ($self, $length, $iteratee) = @_;
  	(defined($length) && defined($iteratee))
          || die "Insufficient Arguments : you must provide an length and an iteratee";
  	$self->{_current_index} = 0;
  	$self->{_length} = $length;
  	$self->{_iteratee} = $iteratee;
  }
  
