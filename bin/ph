      qr{^PATCH /repos/[^/]+/[^/]+/git/refs/.*$},
      qr{^PATCH /repos/[^/]+/[^/]+/hooks/.*$},
      qr{^PATCH /repos/[^/]+/[^/]+/issues/.*$},
