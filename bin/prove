  L<TAP::Parser::SourceHandler::Perl>,
  L<TAP::Parser::SourceHandler::File>,
  L<TAP::Parser::SourceHandler::Handle>,
  L<TAP::Parser::SourceHandler::RawTAP>
  
  =cut
  
