      my $reactor = shift;
      $reactor->remove($first);
      $reactor->remove($second);
      say 'Timeout!';
    });
  
    # Start reactor if necessary
    $reactor->start unless $reactor->is_running;
  
  =head1 DESCRIPTION
  
