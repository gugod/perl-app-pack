  use Mojo::Util qw(quote split_header);
  
  sub parse {
    my ($self, $str) = @_;
  
    my @cookies;
    my @pairs = map {@$_} @{split_header $str // ''};
    while (my ($name, $value) = splice @pairs, 0, 2) {
