  use Mojo::Base 'Mojo::IOLoop::Stream::HTTPClient';
  
  use Scalar::Util 'weaken';
  
  sub process {
    my ($self, $tx) = @_;
    $self->{tx} = $tx;
    weaken $self;
    $tx->on(resume => sub { $self->_write_content });
    $self->_write_content;
  }
  
