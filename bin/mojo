    return $self->_test('like', $self->tx->res->text,
      $regex, _desc($desc, 'content is similar'));
  }
  
  sub content_type_is {
    my ($self, $type, $desc) = @_;
    $desc = _desc($desc, "Content-Type: $type");
    return $self->_test('is', $self->tx->res->headers->content_type, $type,
